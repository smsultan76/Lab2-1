import java.util.Scanner;
public class ConditonalStatement {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int x=20,y=10,n,i;
        System.out.println("Initialixed x=20 and y=10\n");
        //if statement.
        if(x!=y) System.out.println("x and y are not equal!");      //true
        //if-else statement.
        if(x>y) System.out.println("x is grater than y.");          //true
        else System.out.println("y is grater than xf");             //false
        //switch case statement.
        n=sc.nextInt();
        switch(n){
            case 1 -> System.out.println("Entered value= 1");
            case 2 -> System.out.println("Entered value= 2");
            case 3 -> System.out.println("Entered value= 3");
            case 4 -> System.out.println("Entered value= 4");
            case 5 -> System.out.println("Entered value= 5");
            case 6 -> System.out.println("Entered value= 6");
            default -> System.out.println("Not value from 1-6");
        }
        //print 1-5 using for loop.
        for(i=1;i<=5;i++)System.out.print(i+"\t");
        //print 5-10 using while loop.
        while(i!=11){
           System.out.print(i+"\t");
           i++;
        }
        //print 10-15 using do-while loop.
        do{
            System.out.print(i+"\t");
            i++;
        }while(i<=15);
    }
}
